{
  "name": "ProppyApp",
  "version": "0.0.1",
  "author": "Eason Chai <echai2905@gmail.com>",
  "private": true,
  "scripts": {
    "start:backend:dev": "cd backend\\Proppy.API && dotnet watch run",
    "start:backend:prod": "cd backend\\Proppy.API && dotnet run",
    "start:mobile-app": "cd app && ionic capacitor run android -l --external",
    "backend:migrate": "cd backend\\Proppy.API && dotnet ef database update",
    "backend:reset": "cd backend\\Proppy.API && dotnet ef database update 0",
    "backend:migrate:add": "cd backend\\Proppy.API && dotnet ef migrations add",
    "build:android": "cd app && ionic cap copy android"
  },
  "dependencies": {
    "@capacitor/android": "^2.4.7",
    "@capacitor/core": "2.4.7",
    "@ionic/vue": "^5.4.0",
    "@ionic/vue-router": "^5.4.0",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "vue": "^3.0.0-0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "2.4.7",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-0"
  },
  "description": "An Ionic project"
}
